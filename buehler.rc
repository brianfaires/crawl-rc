local function set_all(monster_list, option)
  local mon_str = nil
  for _, v in ipairs(monster_list) do
    if not mon_str then
      mon_str = v
    else
      mon_str = mon_str .. "|" .. v
    end
  end
  set_monster_option("+", mon_str, option)
